!! usage xrdb ~/.Xresources

!! Grayscale xft setting -------------------------------------------------------

Xft.antialias: 1
Xft.dpi:       96
Xft.hinting:   1
!Xft.hintstyle: hintslight
Xft.hintstyle: hintmedium
!Xft.rgba:      none
Xft.rgba:      rgb
Xft.lcdfilter: lcdlight
!Xft.lcdfilter: lcddefault
Xft.autohint:  0

#include "Xresources.urxvt"
#include "Xresources.urxvt.solarized"
